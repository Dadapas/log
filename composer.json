{
    "name": "dadapas/log",
    "description": "Common libraries implementating psr-3 logging interface",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "log",
        "psr",
        "psrThree"
    ],
    "autoload": {
        "psr-4": {
            "Dadapas\\Log\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dadapas\\LogTests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "TOVOHERY",
            "email": "tovoherypascal@gmail.com"
        }
    ],
    "provide": {
        "psr/log": "1.1"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=7.2",
        "psr/log": "^1.1",
        "phpmailer/phpmailer": "^6.6",
        "league/flysystem": "^2.4"
    },
    "version": "1.1",
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit ."
        ]
    }
}
